<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none"
    android:fillViewport="true"

    android:background="#242A37"
    tools:context=".views.setting_actevety">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/paent_constrain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layoutDirection="ltr"
        >
        <TextView
            android:id="@+id/arrrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_close"
            android:layoutDirection="ltr"
            app:layout_constraintTop_toTopOf="@id/paent_constrain"
            android:layout_marginTop="30dp"
            app:layout_constraintStart_toStartOf="@id/paent_constrain"
            android:layout_marginStart="22dp"/>
        <TextView
            android:id="@+id/setting"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@id/arrrow"
            android:text="@string/setting"
            android:textColor="#FFFFFF"
            android:textSize="34sp"
            app:layout_constraintStart_toStartOf="@id/arrrow"/>
        <TableRow
            android:id="@+id/tableraw"
            android:layout_height="1dp"
            android:layout_width="0dp"
            app:layout_constraintStart_toStartOf="@id/paent_constrain"
            app:layout_constraintEnd_toEndOf="@id/paent_constrain"
            android:layout_marginTop="20dp"
            android:background="#000000"
            app:layout_constraintTop_toBottomOf="@id/setting"
            />
        <TextView
            android:id="@+id/txt1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/tableraw"
            app:layout_constraintStart_toStartOf="@id/setting"
            android:layout_marginStart="20dp"
            android:text="@string/edit_profil"
           android:layout_marginTop="20dp"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            />
        <TableRow
            android:id="@+id/tableraw1"
            android:layout_height="3dp"
            android:layout_width="0dp"
            app:layout_constraintStart_toStartOf="@id/txt1"
            app:layout_constraintEnd_toEndOf="@id/paent_constrain"
            android:layout_marginTop="20dp"
            android:background="#222834"
            app:layout_constraintTop_toBottomOf="@id/txt1"
            />
        <TextView
            android:id="@+id/roooooo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:background="@drawable/ic_arowwwwww"
            app:layout_constraintTop_toTopOf="@id/tableraw"
            app:layout_constraintBottom_toBottomOf="@id/tableraw1"
            android:layout_marginEnd="25dp"
            app:layout_constraintEnd_toEndOf="@id/paent_constrain"/>
        <TextView
            android:id="@+id/txt2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/tableraw1"
            app:layout_constraintStart_toStartOf="@id/setting"
            android:layout_marginStart="20dp"
            android:text="@string/languag"
            android:layout_marginTop="20dp"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            />
        <TableRow
            android:id="@+id/tableraw2"
            android:layout_height="3dp"
            android:layout_width="0dp"
            app:layout_constraintStart_toStartOf="@id/txt1"
            app:layout_constraintEnd_toEndOf="@id/paent_constrain"
            android:layout_marginTop="20dp"
            android:background="#222834"
            app:layout_constraintTop_toBottomOf="@id/txt1"
            />
        <TextView
            android:id="@+id/language"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_arowwwwww"
            app:layout_constraintTop_toTopOf="@id/tableraw1"
            app:layout_constraintBottom_toBottomOf="@id/tableraw4"
            android:layout_marginEnd="25dp"
            app:layout_constraintEnd_toEndOf="@id/paent_constrain"/>
        <TableRow
            android:id="@+id/tableraw4"
            android:layout_height="3dp"
            android:layout_width="0dp"
            app:layout_constraintStart_toStartOf="@id/txt1"
            app:layout_constraintEnd_toEndOf="@id/paent_constrain"
            android:layout_marginTop="20dp"
            android:background="#222834"
            app:layout_constraintTop_toBottomOf="@id/txt2"
            />
        <TextView
            android:id="@+id/txt6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:text="@string/contact"
            android:layout_marginTop="20dp"

            android:textColor="#FFFFFF"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="@id/txt2"
            app:layout_constraintTop_toBottomOf="@id/tableraw4"
            />
        <TextView
            android:id="@+id/contact_us_action"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_arowwwwww"
            app:layout_constraintTop_toTopOf="@id/tableraw4"
            app:layout_constraintBottom_toBottomOf="@id/tableraw5"
            android:layout_marginEnd="25dp"

            app:layout_constraintEnd_toEndOf="@id/paent_constrain"/>
        <TableRow
            android:id="@+id/tableraw5"
            android:layout_height="3dp"
            android:layout_width="0dp"
            app:layout_constraintStart_toStartOf="@id/txt1"
            app:layout_constraintEnd_toEndOf="@id/paent_constrain"
            android:layout_marginTop="20dp"
            android:background="#222834"
            app:layout_constraintTop_toBottomOf="@id/txt6"/>
        <TextView
            android:id="@+id/txt7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@id/tableraw5"
            app:layout_constraintStart_toStartOf="@id/txt6"
            android:text="@string/privacy_polic"

            android:textColor="#FFFFFF"
            android:textSize="20sp"/>
        <TextView
            android:id="@+id/privacy_policy"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_arowwwwww"
            app:layout_constraintTop_toTopOf="@id/tableraw5"
            app:layout_constraintBottom_toBottomOf="@id/tableraw6"
            android:layout_marginEnd="25dp"

            app:layout_constraintEnd_toEndOf="@id/paent_constrain"/>
        <TableRow
            android:id="@+id/tableraw6"
            android:layout_height="3dp"
            android:layout_width="0dp"
            app:layout_constraintStart_toStartOf="@id/txt1"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="@id/paent_constrain"
            android:layout_marginTop="20dp"
            android:background="#222834"
            app:layout_constraintTop_toBottomOf="@id/txt7"/>
        <TextView
            android:id="@+id/txt8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@id/tableraw6"
            app:layout_constraintStart_toStartOf="@id/txt6"
            android:text="@string/search_histor"


            android:textColor="#FFFFFF"
            android:textSize="20sp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_arowwwwww"
            android:visibility="gone"
            app:layout_constraintTop_toTopOf="@id/tableraw6"
            app:layout_constraintBottom_toBottomOf="@id/tableraw7"
            android:layout_marginEnd="25dp"

            app:layout_constraintEnd_toEndOf="@id/paent_constrain"/>
        <TableRow
            android:id="@+id/tableraw7"
            android:layout_height="3dp"
            android:layout_width="0dp"
            app:layout_constraintStart_toStartOf="@id/txt1"
            app:layout_constraintEnd_toEndOf="@id/paent_constrain"
            android:layout_marginTop="20dp"

            android:background="#222834"
            app:layout_constraintTop_toBottomOf="@id/txt8"/>
        <TextView
            android:id="@+id/txt9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"

            app:layout_constraintTop_toBottomOf="@id/tableraw7"
            app:layout_constraintStart_toStartOf="@id/txt6"
            android:text="@string/personal_profilesssss"

            android:textColor="#FFFFFF"
            android:textSize="20sp"
            />
        <TextView
            android:id="@+id/nobsa_prof"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_arowwwwww"
            app:layout_constraintTop_toTopOf="@id/tableraw7"
            app:layout_constraintBottom_toBottomOf="@id/tableraw8"

            android:layout_marginEnd="25dp"

            app:layout_constraintEnd_toEndOf="@id/paent_constrain"/>
        <TableRow
            android:id="@+id/tableraw8"
            android:layout_height="3dp"
            android:layout_width="0dp"
            app:layout_constraintStart_toStartOf="@id/txt1"

            app:layout_constraintEnd_toEndOf="@id/paent_constrain"
            android:layout_marginTop="20dp"
            android:background="#222834"
            app:layout_constraintTop_toBottomOf="@id/txt9"/>
        <TableRow
            android:layout_height="7dp"
            android:layout_width="135dp"
            android:background="#FFFFFF"
            android:visibility="gone"
            android:layout_marginBottom="7dp"

            app:layout_constraintBottom_toBottomOf="parent"

            app:layout_constraintStart_toStartOf="@id/paent_constrain"
            app:layout_constraintEnd_toEndOf="@id/paent_constrain"
            />
        <TextView
            android:id="@+id/txt25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@id/tableraw8"
            app:layout_constraintStart_toStartOf="@id/txt6"
            android:text="@string/change_passwrd"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            />
        <TextView
            android:id="@+id/table_raw"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_arowwwwww"
            app:layout_constraintTop_toTopOf="@id/tableraw8"
            app:layout_constraintBottom_toBottomOf="@id/tableraw9"
            android:layout_marginEnd="25dp"

            app:layout_constraintEnd_toEndOf="@id/paent_constrain"/>
        <TableRow
            android:id="@+id/tableraw9"
            android:layout_height="3dp"
            android:layout_width="0dp"
            app:layout_constraintStart_toStartOf="@id/txt1"
            app:layout_constraintEnd_toEndOf="@id/paent_constrain"
            android:layout_marginTop="20dp"
            android:background="#222834"
            app:layout_constraintTop_toBottomOf="@id/txt25"/>
        <TextView
            android:id="@+id/txt26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@id/tableraw9"
            app:layout_constraintStart_toStartOf="@id/txt6"
            android:text="@string/logout"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            />
        <TextView
            android:id="@+id/table_raw1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_arowwwwww"
            app:layout_constraintTop_toTopOf="@id/txt26"
            app:layout_constraintBottom_toBottomOf="@id/tableraw10"
            android:layout_marginEnd="25dp"

            app:layout_constraintEnd_toEndOf="@id/paent_constrain"/>
        <TableRow
            android:id="@+id/tableraw10"
            android:layout_height="3dp"
            android:layout_width="0dp"
            app:layout_constraintStart_toStartOf="@id/txt1"
            app:layout_constraintEnd_toEndOf="@id/paent_constrain"
            android:layout_marginTop="20dp"
            android:background="#222834"
            app:layout_constraintTop_toBottomOf="@id/txt26"/>
        <com.google.android.gms.ads.AdView
            android:id="@+id/adView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"


            app:adSize="BANNER"
            app:adUnitId="ca-app-pub-8351929453534437/4967121882"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            />










    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>